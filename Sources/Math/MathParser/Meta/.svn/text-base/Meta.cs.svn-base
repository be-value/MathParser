namespace MathParser
{
    /// <summary>
    /// The type of meta token
    /// </summary>
    internal enum Meta
    {
        /// <summary>
        /// Represents an left parenthesis token
        /// </summary>
        LeftParenthesis,

        /// <summary>
        /// Represents an right parenthesis token
        /// </summary>
        RightParenthesis,

        /// <summary>
        /// Representa an comma token
        /// </summary>
        Comma
    }
}
